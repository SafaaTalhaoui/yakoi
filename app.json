{
  "expo": {
    "name": "Yakoi",
    "slug": "yakoi",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "4.0.0",
    "orientation": "portrait",
    "icon": "./assets/yakoi-icon.png",
    "splash": {
      "image": "./assets/yakoi-splash.png",
      "resizeMode": "contain",
